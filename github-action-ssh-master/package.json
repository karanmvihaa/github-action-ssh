{
  "name": "@garygrossgarten/github-action-ssh",
  "version": "0.7.0",
  "description": "Run commands on a remote server via SSH.",
  "repository": {
    "type": "git",
    "url": "https://github.com/garygrossgarten/github-action-ssh"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build --source-map --license licenses.txt"
  },
  "bin": {
    "github-action-ssh": "dist/index.js"
  },
  "preferGlobal": false,
  "keywords": [
    "typescript",
    "github",
    "github-actions",
    "actions",
    "ssh"
  ],
  "author": "garygrossgarten",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "node-ssh": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/ssh2": "^1.11.6",
    "@types/ssh2-streams": "^0.1.9",
    "@vercel/ncc": "^0.34.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  }
}
